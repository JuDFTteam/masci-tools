    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "import panel as pn\n",
    "pn.extension()\n",
    "\n",
    "def plot(minimum, maximum):\n",
    "    x = np.arange(minimum, maximum)\n",
    "    plt.plot(x,3*x)\n",
    "    return plt.gcf()\n",
    "    \n",
    "pn.interact(plot, minimum=0, maximum=10)"
   ]
